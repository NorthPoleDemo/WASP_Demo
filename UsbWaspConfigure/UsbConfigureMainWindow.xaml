<Window x:Class="UsbWaspConfigure.UsbConfigureMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UsbWaspConfigure"
        mc:Ignorable="d"
        Title="Configure WASP-N" Height="350" Width="525">
    <Grid>
        <Label x:Name="label" Content="SSID" HorizontalAlignment="Left" Margin="64,79,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Password" HorizontalAlignment="Left" Margin="223,79,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxSsid" Text="ssid!" HorizontalAlignment="Left" Height="23" Margin="64,110,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="223,110,0,0" VerticalAlignment="Top" Width="142" Height="23"/>
        <Button x:Name="buttonConf" Content="Store" HorizontalAlignment="Left" Margin="64,164,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False" Click="buttonConf_Click"/>

    </Grid>
</Window>
